<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.raul.circuito">

    <application
        android:name=".Clases.GlobalClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TabbedToolsActivity"
            android:label="@string/title_activity_tabbed_tools"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TabbedConsActivity"
            android:label="@string/title_activity_tabbed_cons"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".GraficoActivity" />
        <activity
            android:name=".TabbedIngresoActivity"
            android:label="@string/title_activity_tabbed_ingreso"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>